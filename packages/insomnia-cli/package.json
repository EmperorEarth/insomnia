{
  "name": "insomnia-cli",
  "version": "1.0.0",
  "main": "dist/index.js",
  "bin": {
    "insomnia": "dist/index.js"
  },
  "scripts": {
    "typecheck": "flow check",
    "test": "jest",
    "start": "npm run watch",
    "build": "babel ./src --out-dir=./dist",
    "watch": "babel --source-maps --watch=./src --out-dir=./dist",
    "bootstrap": "npm run build",
    "prepublish": "npm run build"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "verbose": false,
    "resetMocks": true,
    "resetModules": true
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-flow": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "husky": "^3.1.0",
    "ink": "^2.7.1",
    "react": "^16.13.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "commander": "^5.1.0"
  }
}
