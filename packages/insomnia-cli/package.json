{
  "name": "insomnia-cli",
  "version": "0.0.1",
  "main": "dist/inso.js",
  "bin": {
    "inso": "dist/inso.js"
  },
  "scripts": {
    "typecheck": "",
    "test": "jest",
    "test:snapshots": "jest -u",
    "start": "npm run watch",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir=./dist --ignore '**/*/__tests__/**'",
    "watch": "babel --source-maps --watch=./src --out-dir=./dist --ignore '**/*/__tests__/**'",
    "bootstrap": "npm run build",
    "prepublish": "npm run build"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "verbose": false,
    "resetMocks": true,
    "resetModules": true
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-flow": "^7.10.1",
    "execa-wrap": "^1.4.0",
    "flow-bin": "^0.126.1",
    "husky": "^3.1.0",
    "jest": "^26.0.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "commander": "^5.1.0",
    "openapi-2-kong": "^2.2.6",
    "yaml": "^1.10.0"
  }
}
